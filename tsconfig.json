{
    "compilerOptions": {
      "esModuleInterop": true,
      "isolatedModules": true,
      "noImplicitAny": false,
      "strictNullChecks": false,
      "lib": [
        "esnext"
      ],
      "module": "esnext",
      "moduleResolution": "node",
      "noUncheckedIndexedAccess": true,
      "outDir": "dist",
      "strict": true,
      "target": "esnext",
      "useUnknownInCatchVariables": true
    },
    "include": [
      "my-firewalla/**/*"
    ]
}